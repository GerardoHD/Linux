#Descargar-Scripts
echo          ------==Script Creado por GerardoHD==------
echo       ---=Descargando Scripts=---
sleep 3s
echo
#!/bin/bash
cd /usr/local/bin
sudo rm -rf screenfetch
sudo rm -rf Actualizar-Arch
sudo rm -rf Actualizar-Ubuntu
sudo rm -rf Arch-Setup
sudo rm -rf Blue-Script
sudo rm -rf Descargar-Scripts
sudo rm -rf Limpiar-Arch
sudo rm -rf Limpiar-Ubuntu
sudo rm -rf Materia-GDM-Theme
sudo rm -rf Original-GDM-Theme
sudo rm -rf Poweroff
sudo rm -rf ROOT-A
sudo rm -rf Reboot
sudo rm -rf Red-Script
sudo rm -rf Reparar-Pacman-db.lck
sudo rm -rf Reparar-var-Ubuntu
sudo rm -rf STATE
sudo rm -rf Tweaks
sudo rm -rf Ubuntu-Setup
sudo wget -c https://raw.githubusercontent.com/KittyKatt/screenFetch/master/screenfetch-dev -O screenfetch
sudo wget -c https://raw.githubusercontent.com/GerardoHD/Linux/master/Scripts/Arch/Actualizar-Arch
sudo wget -c https://raw.githubusercontent.com/GerardoHD/Linux/master/Scripts/Actualizar-Ubuntu
sudo wget -c https://raw.githubusercontent.com/GerardoHD/Linux/master/Scripts/Arch/Arch-Setup
sudo wget -c https://raw.githubusercontent.com/GerardoHD/Linux/master/Scripts/Blue-Script
sudo wget -c https://raw.githubusercontent.com/GerardoHD/Linux/master/Scripts/Descargar-Scripts
sudo wget -c https://raw.githubusercontent.com/GerardoHD/Linux/master/Scripts/Arch/Limpiar-Arch
sudo wget -c https://raw.githubusercontent.com/GerardoHD/Linux/master/Scripts/Limpiar-Ubuntu
sudo wget -c https://raw.githubusercontent.com/GerardoHD/Linux/master/Scripts/Materia-GDM-Theme
sudo wget -c https://raw.githubusercontent.com/GerardoHD/Linux/master/Scripts/Original-GDM-Theme
sudo wget -c https://raw.githubusercontent.com/GerardoHD/Linux/master/Scripts/Arch/Poweroff
sudo wget -c https://raw.githubusercontent.com/GerardoHD/Linux/master/Scripts/ROOT-A
sudo wget -c https://raw.githubusercontent.com/GerardoHD/Linux/master/Scripts/Arch/Reboot
sudo wget -c https://raw.githubusercontent.com/GerardoHD/Linux/master/Scripts/Red-Script
sudo wget -c https://raw.githubusercontent.com/GerardoHD/Linux/master/Scripts/Arch/Reparar-Pacman-db.lck
sudo wget -c https://raw.githubusercontent.com/GerardoHD/Linux/master/Scripts/Reparar-var-Ubuntu
sudo wget -c https://raw.githubusercontent.com/GerardoHD/Linux/master/Scripts/STATE
sudo wget -c https://raw.githubusercontent.com/GerardoHD/Linux/master/Scripts/Arch/Tweaks
sudo wget -c https://raw.githubusercontent.com/GerardoHD/Linux/master/Scripts/Ubuntu-Setup
sudo chmod +x screenfetch
sudo chmod +x Actualizar-Arch
sudo chmod +x Actualizar-Ubuntu
sudo chmod +x Arch-Setup
sudo chmod +x Blue-Script
sudo chmod +x Descargar-Scripts
sudo chmod +x Limpiar-Arch
sudo chmod +x Limpiar-Ubuntu
sudo chmod +x Materia-GDM-Theme
sudo chmod +x Original-GDM-Theme
sudo chmod +x Poweroff
sudo chmod +x ROOT-A
sudo chmod +x Reboot
sudo chmod +x Red-Script
sudo chmod +x Reparar-Pacman-db.lck
sudo chmod +x Reparar-var-Ubuntu
sudo chmod +x STATE
sudo chmod +x Tweaks
sudo chmod +x Ubuntu-Setup
cd /
sleep 1s
echo Ejecuta el Script Elegido
echo Recomiendo revisar alguno de los scripts ubicados en:
echo /usr/local/bin/
ls /usr/local/bin/
sleep 5s
clear
screenfetch
